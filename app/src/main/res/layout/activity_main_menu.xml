<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#011127"
    tools:context=".MainMenu">

    <TextView
        android:id="@+id/txtLogout"
        android:layout_width="63dp"
        android:layout_height="21dp"
        android:layout_marginTop="8dp"
        android:text="Logout"
        android:textAlignment="center"
        android:textColor="@color/white"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.974"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/img_Exit" />

    <TextView
        android:id="@+id/txtAnuncio"
        android:layout_width="206dp"
        android:layout_height="36dp"
        android:layout_marginTop="144dp"
        android:fontFamily="casual"
        android:text="¿Qué hay pa' hacer?"
        android:textColor="@color/white"
        android:textSize="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />

    <Button
        android:id="@+id/btnMusica"
        android:layout_width="110dp"
        android:layout_height="50dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="@color/white"
        android:fontFamily="monospace"
        android:text="Música"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/btnCultura"
        app:layout_constraintHorizontal_bias="0.696"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgMusica" />

    <Button
        android:id="@+id/btnCultura"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="20dp"
        android:layout_marginEnd="72dp"
        android:backgroundTint="@color/white"
        android:fontFamily="monospace"
        android:text="Cultura"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView7" />

    <Button
        android:id="@+id/btnRestau"
        android:layout_width="157dp"
        android:layout_height="51dp"
        android:layout_marginTop="28dp"
        android:backgroundTint="@color/white"
        android:fontFamily="monospace"
        android:text="Restaurantes"
        android:textColor="@color/black"
        app:layout_constraintEnd_toStartOf="@+id/btnDeportes"
        app:layout_constraintHorizontal_bias="0.826"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imgRestaurantes" />

    <Button
        android:id="@+id/btnDeportes"
        android:layout_width="127dp"
        android:layout_height="54dp"
        android:layout_marginTop="28dp"
        android:layout_marginEnd="52dp"
        android:backgroundTint="@color/white"
        android:fontFamily="monospace"
        android:text="Deportes"
        android:textColor="@color/black"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageView9" />

    <ImageView
        android:id="@+id/imgMusica"
        android:layout_width="91dp"
        android:layout_height="97dp"
        android:layout_marginStart="92dp"
        android:layout_marginTop="44dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/txtAnuncio"
        app:srcCompat="@drawable/music" />

    <ImageView
        android:id="@+id/imgRestaurantes"
        android:layout_width="95dp"
        android:layout_height="115dp"
        android:layout_marginStart="88dp"
        android:layout_marginTop="24dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnMusica"
        app:srcCompat="@drawable/comidas" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="100dp"
        android:layout_height="103dp"
        android:layout_marginTop="36dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.437"
        app:layout_constraintStart_toEndOf="@+id/imgRestaurantes"
        app:layout_constraintTop_toBottomOf="@+id/btnCultura"
        app:srcCompat="@drawable/deportes" />

    <ImageView
        android:id="@+id/img_Exit"
        android:layout_width="46dp"
        android:layout_height="40dp"
        android:layout_marginTop="20dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.956"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/btnDeportes"
        app:srcCompat="@drawable/logout" />

    <ImageView
        android:id="@+id/logo1"
        android:layout_width="98dp"
        android:layout_height="81dp"
        android:layout_marginTop="24dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.916"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/logosinnombre" />

    <ImageView
        android:id="@+id/imageView7"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="44dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.507"
        app:layout_constraintStart_toEndOf="@+id/imgMusica"
        app:layout_constraintTop_toBottomOf="@+id/txtAnuncio"
        app:srcCompat="@drawable/bandera" />


</androidx.constraintlayout.widget.ConstraintLayout>